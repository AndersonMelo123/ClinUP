<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".activity.CancelarAgendamentos">

    <TextView
        android:id="@+id/textView16"
        android:layout_width="150dp"
        android:layout_height="25dp"
        android:layout_marginBottom="8dp"
        android:layout_marginStart="28dp"
        android:layout_marginTop="8dp"
        android:text="Data da Consulta:"
        android:textColor="@color/Black"
        android:textSize="18sp"
        android:visibility="visible"
        app:layout_constraintBottom_toTopOf="@+id/dataConsulta"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/dataAgendamento"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/textView14"
        android:layout_width="60dp"
        android:layout_height="25dp"
        android:layout_marginBottom="12dp"
        android:layout_marginStart="28dp"
        android:text="Nome:"
        android:textColor="@color/Black"
        android:textSize="18sp"
        android:visibility="visible"
        app:layout_constraintBottom_toTopOf="@+id/nome_l"
        app:layout_constraintStart_toStartOf="parent" />

    <include
        android:id="@+id/toolbarActivity"
        layout="@layout/toolbar_activity"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="40dp"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/nome_l"
        android:layout_width="322dp"
        android:layout_height="39dp"
        android:layout_marginTop="120dp"
        android:background="#4d000000"
        android:gravity="center"
        android:textAlignment="inherit"
        android:textColor="@color/White"
        android:textSize="18sp"
        android:theme="@style/StyleEditText"
        android:visibility="visible"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/dataAgendamento"
        android:layout_width="322dp"
        android:layout_height="39dp"
        android:layout_marginTop="112dp"
        android:background="#4d000000"
        android:gravity="center"
        android:textAlignment="center"
        android:textColor="@color/White"
        android:textSize="18sp"
        android:theme="@style/StyleEditText"
        android:visibility="visible"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/nome_l" />

    <TextView
        android:id="@+id/dataConsulta"
        android:layout_width="322dp"
        android:layout_height="39dp"
        android:layout_marginTop="192dp"
        android:background="#4d000000"
        android:gravity="center"
        android:textAlignment="center"
        android:textColor="@color/White"
        android:textSize="18sp"
        android:theme="@style/StyleEditText"
        android:visibility="visible"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/nome_l" />

    <TextView
        android:id="@+id/planoSaud"
        android:layout_width="322dp"
        android:layout_height="39dp"
        android:layout_marginTop="32dp"
        android:background="#4d000000"
        android:gravity="center"
        android:textAlignment="center"
        android:textColor="@color/White"
        android:textSize="18sp"
        android:theme="@style/StyleEditText"
        android:visibility="visible"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/nome_l" />

    <Button
        android:id="@+id/cancelarAgendamento"
        android:layout_width="0dp"
        android:layout_height="45dp"
        android:layout_marginBottom="72dp"
        android:text="@string/botao_cancelar"
        android:theme="@style/botaoPadrao"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="130dp"
        android:layout_height="25dp"
        android:layout_marginBottom="8dp"
        android:layout_marginStart="28dp"
        android:layout_marginTop="8dp"
        android:text="Plano de SaÃºde:"
        android:textColor="@color/Black"
        android:textSize="18sp"
        android:visibility="visible"
        app:layout_constraintBottom_toTopOf="@+id/planoSaud"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/nome_l"
        app:layout_constraintVertical_bias="0.888" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="200dp"
        android:layout_height="25dp"
        android:layout_marginBottom="8dp"
        android:layout_marginStart="28dp"
        android:layout_marginTop="8dp"
        android:text="Data do Agendamento:"
        android:textColor="@color/Black"
        android:textSize="18sp"
        android:visibility="visible"
        app:layout_constraintBottom_toTopOf="@+id/dataAgendamento"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/planoSaud"
        app:layout_constraintVertical_bias="1.0" />

</android.support.constraint.ConstraintLayout>